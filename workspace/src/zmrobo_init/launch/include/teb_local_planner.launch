<launch>

  <!-- navigation common params-->
  <rosparam file="$(find zmrobo_init)/param_common/local_costmap_params.yaml"      command="load" ns="move_base"/>
  <rosparam file="$(find zmrobo_init)/param_common/global_costmap_params.yaml"     command="load" ns="move_base"/>
  <rosparam file="$(find zmrobo_init)/param_common/move_base_params.yaml"          command="load" ns="move_base"/>
  <rosparam file="$(find zmrobo_init)/param_common/base_global_planner_param.yaml" command="load" ns="move_base"/>
  <rosparam file="$(find zmrobo_init)/param_common/teb_local_planner_params.yaml"  command="load" ns="move_base"/>

  <param name="move_base/base_local_planner"    type="string" value="teb_local_planner/TebLocalPlannerROS"/>


  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find zmrobo_init)/costmap_common_params/param_mini_mec/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find zmrobo_init)/costmap_common_params/param_mini_mec/costmap_common_params.yaml" command="load" ns="local_costmap" />
  </node>

  
</launch>


