
<launch>
 <!-- 开启机器人底层相关节点  -->
 <include file="$(find zmrobo_init)/launch/zmrobo_init.launch" />
 <!-- turn on lidar开启思岚雷达  -->
 <include file="$(find lslidar_x10_driver)/launch/zmrobo_laser.launch" />

 <!-- 设置需要用于导航的地图  -->
 <arg name="map_file" default="$(find zmrobo_init)/map/ZMROBO.yaml"/>
 <node name="map_server_for_test" pkg="map_server" type="map_server" args="$(arg map_file)">
 </node>

 <!-- 开启用于导航的自适应蒙特卡洛定位amcl_omni  运动方式是omni-->
 <include file="$(find zmrobo_init)/launch/include/amcl_omni.launch" />

 <!-- 开启teb_local_planner导航算法-->
 <include file="$(find zmrobo_init)/launch/include/teb_local_planner.launch" />
 <!-- 开启dwa_local_planner导航算法-->
 <!-- <include file="$(find zmrobo_init)/launch/include/dwa_local_planner.launch" /> -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find zmrobo_init)/config/zmrobo.rviz" required="true" />

</launch>




